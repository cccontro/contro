        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:range rdf:resource="https://w3id.org/contro/arg#Conclusion"/>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/contro/arg#InferenceRule"/>
            <rdf:Description rdf:about="https://w3id.org/contro/arg#Consequent"/>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/contro/arg#Consequent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/contro/arg#Consequent">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:range rdf:resource="https://w3id.org/contro/arg#Consequent"/>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/contro/arg#DialogicalAgent -->


    <owl:ObjectProperty rdf:about="https://w3id.org/contro/arg#DialogicalAgent">
        <rdfs:subPropertyOf rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#isSettingFor"/>
        <rdfs:domain rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Situation"/>
        <rdfs:range rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#Agent"/>
        <rdfs:range>
            <owl:Restriction>
                <owl:onProperty rdf:resource="http://www.ontologydesignpatterns.org/ont/dul/DUL.owl#hasRole"/>
                <owl:hasValue rdf:resource="https://w3id.org/contro/arg#DialogicalAgent"/>
            </owl:Restriction>
        </rdfs:range>
        <owl:propertyChainAxiom rdf:parseType="Collection">
            <rdf:Description rdf:about="https://w3id.org/contro/arg#DialogicalAgent"/>
            <rdf:Description>
                <owl:inverseOf rdf:resource="https://w3id.org/contro/arg#isAliasOf"/>
            </rdf:Description>
        </owl:propertyChainAxiom>
    </owl:ObjectProperty>
    


    <!-- https://w3id.org/contro/arg#InferenceRule -->


