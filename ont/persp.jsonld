    "@id" : "http://www.ontologydesignpatterns.org/ont/persp/perspectivisation.owl"
  } ],
  "http://purl.org/dc/terms/title" : [ {
    "@language" : "en",
    "@value" : "Perspectivisation Ontology"
  } ],
  "http://purl.org/vocab/vann/preferredNamespacePrefix" : [ {
    "@value" : "persp"
  } ],
  "http://purl.org/vocab/vann/preferredNamespaceUri" : [ {
    "@id" : "https://w3id.org/contro/persp#"
  } ],
