###  https://w3id.org/contro#Conclusion
:Conclusion rdf:type owl:ObjectProperty .


###  https://w3id.org/contro#Implication
:Implication rdf:type owl:ObjectProperty .


###  https://w3id.org/contro#Premise
:Premise rdf:type owl:ObjectProperty .


#################################################################
#    Classes
#################################################################

###  https://w3id.org/contro#AffirmativeArgumentation
:AffirmativeArgumentation rdf:type owl:Class ;
                          owl:equivalentClass [ owl:intersectionOf ( :Argumentation
                                                                     [ rdf:type owl:Restriction ;
                                                                       owl:onProperty :Conclusion ;
                                                                       owl:someValuesFrom :Statement
                                                                     ]
                                                                   ) ;
                                                rdf:type owl:Class
                                              ] .


###  https://w3id.org/contro#Argumentation
:Argumentation rdf:type owl:Class .


###  https://w3id.org/contro#Assumption
:Assumption rdf:type owl:Class ;
            rdfs:subClassOf :Premise ;
            owl:disjointWith :Conclusion .


###  https://w3id.org/contro#Conclusion
:Conclusion rdf:type owl:Class ;
            rdfs:subClassOf :Statement ;
            owl:hasKey ( :Conclusion
                       ) .


###  https://w3id.org/contro#Conflict
:Conflict rdf:type owl:Class ;
          rdfs:subClassOf :Argumentation .


###  https://w3id.org/contro#DefeasibleInference
:DefeasibleInference rdf:type owl:Class ;
                     rdfs:subClassOf :Argumentation ,
                                     [ rdf:type owl:Restriction ;
                                       owl:onProperty :Conclusion ;
                                       owl:someValuesFrom :Statement
                                     ] .


###  https://w3id.org/contro#Implication
:Implication rdf:type owl:Class ;
             rdfs:subClassOf :Statement ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :Conclusion ;
                               owl:someValuesFrom :Statement
                             ] ,
                             [ rdf:type owl:Restriction ;
                               owl:onProperty :Premise ;
                               owl:someValuesFrom :Statement
                             ] ;
             owl:hasKey ( :Implication
                        ) .


###  https://w3id.org/contro#Premise
:Premise rdf:type owl:Class ;
         rdfs:subClassOf :Statement ;
         owl:hasKey ( :Premise
                    ) .


###  https://w3id.org/contro#Rebuttal
:Rebuttal rdf:type owl:Class ;
          rdfs:subClassOf :Conflict ,
                          [ rdf:type owl:Restriction ;
                            owl:onProperty :Conclusion ;
                            owl:someValuesFrom :Statement
                          ] .


###  https://w3id.org/contro#Statement
:Statement rdf:type owl:Class .


###  https://w3id.org/contro#Undercut
:Undercut rdf:type owl:Class ;
          rdfs:subClassOf :Conflict .


###  https://w3id.org/contro#Undermining
:Undermining rdf:type owl:Class ;
             rdfs:subClassOf :Conflict .


